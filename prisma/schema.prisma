
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Video {
  id     Int @id @default(autoincrement())
  title  String
  path   String
  size   Int
  mimeType String
  status String
  createdAt DateTime @default(now())
  

}